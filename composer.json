{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:komparu/value.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:komparu/unit.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:komparu/resolver.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:komparu/input.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:komparu/document.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:komparu/schema.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:komparu/utility.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/komparu/laravel-mongodb-cache.git"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/lumen-framework": " 5.5.*",
    "vlucas/phpdotenv": "~2.2",
    "barryvdh/laravel-ide-helper": "^2.4",
    "komparu/document": "dev-api2-version",
    "komparu/unit": "dev-master",
    "komparu/utility": "dev-master",
    "komparu/laravel-mongodb-cache": "dev-master",
    "itsgoingd/clockwork": "1.14.*",
    "hannesvdvreken/guzzle-clockwork": "~1.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers/ClockworkHelper.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
